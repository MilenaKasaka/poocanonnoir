Content-Disposition: inline; filename=gabarit-forum.css
Content-Type: text/css; charset=iso-8859-1; name=gabarit-forum.css
Content-Location: http://www.developpez.net/template/gabarit-forum.css
Content-Transfer-Encoding: 8bit

@charset "iso-8859-1";

body { margin: 0; }
img { border: 0; }
#cse-search-box
{ 
	display: block;
} 

img.imgb {
    display: block;
    margin: 0 auto;
}

.copyright {
    color : silver;
    font-size : 10pt;
    font-weight: normal;
}

.dev {
    COLOR: #000000;
    FONT-FAMILY: Verdana, Arial, Helvetica, sans-serif;
    FONT-SIZE: 9pt;
    font-weight: normal;
}

.licence {
    text-align : justify;
    font-size: 7pt;    
    padding-top:2px;
    padding-bottom:16px;
    padding-left:30px;
    padding-right:30px;
}

.titrelarge{
    color : #FFFFFF;
    font-weight : bold;
    font-size : 16pt;
    font-family: Verdana, Arial, Helvetica, sans-serif;
}

a.dico {
    color: #6389D8; 
    text-decoration:none; 
    border-bottom:1px dotted #6389D8;
}

div.contenuPage {
    width: 98%;
    margin: 0 auto;
    font-size: 10pt;
}

div.logoDVP {
    float: left;
    width: 248px;
    position: relative;
    top: -4px;
    position: relative;
    top: -10px;
}

#publicite {
    float: right;
    width: 728px;
    background-color: #E1E1E2;
    text-align: right;
    margin-top: 10px;
    position: relative;
    top: -2px;
}

#pied
{
    margin-top: 10px;
    font-size: 10pt;
    font-family: Verdana, sans-serif;
    text-align: center;
}

div.logoXiTi {
   float: right;
   margin-right: 4px;
}

/** LAYOUT */

#layout
{
    min-width: 975px;

    padding: 0 ;
    margin: auto 10px;
}

#shadow1
{
    background: #FFF url('./images/general/shadow-left.png') left top repeat-y;
    padding-left: 3px;
}

#shadow2
{
    padding-right: 3px;    
    background: #FFF url('./images/general/shadow-right.png') right top repeat-y;
}

div.contenuPage
{
    padding: 5px;
}

/* IE6 Hack */
* html div.contenuPage
{
    margin-top: -12px;
}

/** LAYOUT END */

/** FOOTER */
#gabarit_footer
{
    
    padding: 0;
    margin: 0;
    
    padding-bottom: 2px;

    background-color: #638CDE;
    background-image: url('./images/general/shadow-footer.png');
    background-repeat: repeat-x;
    background-position: left bottom;
    
    height: 32px;
}

#gabarit_footer a
{
    text-decoration: none;
    color: #3964AF;
    font-weight: bold;
}

#gabarit_footer a:hover
{
    text-decoration: underline;
}

#gabarit_footer ul
{
    list-style-type: none;
}

#gabarit_footer *
{
    margin: 0;
    padding: 0;
}

#gabarit_footer #gabarit_footer_left
{
    background-image: url('./images/general/shadow-left.png');
    background-repeat: repeat-y;
    background-position: left;

    float: left;

    width: 4px;
    height: 100%;
}

#gabarit_footer #gabarit_footer_right
{
    background-image: url('./images/general/shadow-right.png');
    background-repeat: repeat-y;
    background-position: right;

    float: right;

    width: 4px;
    height: 100%;
}

#gabarit_footer #responsable
{
    display: block;
    
    font-size: 0.75em;
    text-align: center;

    color: #CCDCF7;
    padding-top: 7px;
}

#gabarit_footer #copyright
{
    float: right;

    color: #FFF;

    font-weight: bold;
    font-size: 0.8em;

    margin-top: 0.4em;

    line-height: 1em;
}

#gabarit_footer a
{
    color: #CCDCF7;
    color: #3964AF;
    color: #FFF;
    font-size: 11px;
}

#gabarit_footer a:hover
{
    text-decoration: underline;
}

#gabarit_footer #sitemap li
{
   
    font-size: 1em;
    font-weight: bold;
    color: #FFF;

    margin: 0.1em;

    display: inline;
}

#gabarit_footer .quicklink
{
    color: #F18F25;
}
/** FOOTER END */

/** QUICKNAV */
#quicknav
{
    width: 835px;
    height: 25px;

    margin: auto;
    
    background: #e1e1e2 url('./images/top-menu.gif') repeat-x 10px bottom ;
}

#quicknav img
{
    border: none;
    vertical-align: middle;
}

#quicknav *
{
    margin: 0;
    padding: 0;
}

#quicknav #sitemap
{
    float: right; 
    
    background: transparent url('./images/top-menu-right.jpg') no-repeat bottom right;
    
	height: 25px;
	
	padding-right: 9px;
	padding-left: 5px;
}

/* IE6 Hack */
* html #quicknav #sitemap
{
	height: 30px;
}

#quicknav #sitemap img
{
    margin-top: -6px;
}

#quicknav #quicksearch
{
    float: right;
}

#quicknav #quicksearch form {
	background: #e1e1e2 url('./images/search.gif') no-repeat bottom left;
	height: 20px;
	padding: 0 5px 5px 0;
	
	width: 195px;
}

#quicknav #quicksearch form #recherche_g
{
    border: 1px solid #DFDFDF;

    font-size: 12px;
   
    color: #B4B3B3;
    
    width: 145px;
    
    margin-left: 5px;
}

#quicknav #quicksearch form #recherche_g:focus
{
    font-style: normal;
    color: #2B2B2B;
}

#quicknav #quicksearch form #recherche_submit
{
	cursor: pointer;
	vertical-align: -2%;
}

#quicknav ul
{
    list-style-type: none;
    
    height: 25px;
	padding: 0 15px;
	background: transparent url('./images/top-menu-left.gif') no-repeat bottom left;
    
    line-height: 17px;
}

/* IE6 Hack */
* html #quicknav ul
{
	height: 25px;
}

#quicknav ul li
{
    display: inline;
    margin-right: 5px;
}

#quicknav li a
{
    text-decoration: none;
    color: #344E81;
    font-weight: bold;
    font-size: 12px;
}

#quicknav li a:hover
{
    text-decoration: underline;
}

/** QUICKNAV END */

/** GABARIT */

#gabarit
{
    clear: both;
}

#gabarit img
{
    vertical-align: middle;
}

#gabarit ul
{
    list-style-type: none;
}

#gabarit a
{
    text-decoration: none;
    color: #3964AF;
    font-weight: bold;
}

#gabarit a:hover
{
    text-decoration: underline;
}

#gabarit abbr
{
    border: none;
}

#gabarit *
{
    margin: 0;
    padding: 0;
}

#gabarit_tabmenu
{
    height: 33px;
   
    margin: 0;

    background-color: #4A638F;
    background-image: url('./images/general/tabmenu-background.png');
    background-repeat: repeat-x;
}

#gabarit_tabmenu div
{
    background-image: url('./images/general/tabmenu-right-background.png');
    background-repeat: no-repeat;

    float: right;

    height: 33px;
    width: 16px;
}

#gabarit_tabmenu #tabs
{
    background-image: url('./images/general/tabmenu-left-background.png');
    background-repeat: no-repeat;

    padding-left: 0.75em;
    
    height: 33px;
}

#gabarit_tabmenu #tabs .first
{
    background-image: none;
    padding: 0;
}

#gabarit_tabmenu #tabs li
{
    float: left;
    position: relative;
    display: block;
    
    font-size: 13px;
    line-height: 35px;
    color: #D8D7D7;
    
    z-index: 100;
    
    height: 33px;

    margin-right: 4px;
    padding-left: 4px;

    background-image: url('./images/general/tabmenu-separator.png');
    background-repeat: no-repeat;
}

/* IE7 Hack */
*+html #gabarit_tabmenu #tabs li
{
    line-height: 36px;
}

#gabarit_tabmenu #tabs li a
{
    font-size: 1em;
    color: #FFF;
    
    padding: 0 0.25em;
    display: block;
    
    height: 100%;
}

/* Ncessaire pour IE6 ! */
* html #gabarit_tabmenu #tabs li a
{
	display: inline-block;
}

#gabarit_tabmenu #tabs li:hover > a
{
    background-image: url('./images/general/tabmenu-background-over.png');
    background-repeat: repeat-x;
}

#gabarit_tabmenu #tabs li a:hover
{
    text-decoration: none;

    background-image: url('./images/general/tabmenu-background-over.png');
    background-repeat: repeat-x;
}

#gabarit_tabmenu #tabs li a.selected
{
    background-image: url('./images/general/tabmenu-background-over.png');
    background-repeat: repeat-x;
}

#gabarit_tabmenu #tabs li:hover a
{
    color: #FFF;
}

#gabarit_tabmenu #tabs li a .tooltip
{
    display: none;
}

#gabarit_tabmenu #tabs li a:hover .tooltip
{
    display: block;
    position: absolute;
    
    z-index: 555;
    
    color: #25457D;
    
    padding-left: 5px;
    
    border: 1px solid #bbb;
    background: #ffc;
    
    font-weight: normal;
    font-size: 10px;
    
    width: 256px;
    height: 18px;
    line-height: 18px;
    top: -19px;
}

#gabarit_tabmenu #tabs li.last a:hover .tooltip
{
    left: -110px;
    width: 185px;
}

#gabarit_tabmenu #tabs li.prelast a:hover .tooltip
{
    width: 150px;
}

*+html #gabarit_tabmenu #tabs li a:hover .tooltip
{
    left: 4px;
}

#gabarit_tabmenu #tabs li ul.submenu
{
	display: none;
}

#gabarit_tabmenu #tabs li:hover a + ul.submenu
{
    display: block;
    position: absolute;

    top: 33px;

    width: 185px;

    border: 1px solid #CCCCCC;
    border-bottom: 2px solid #3964AF;
    background-color: #F9F9F9;
}

/* IE7 Hack */
*+html #gabarit_tabmenu #tabs li:hover a + ul.submenu
{
    top: 33px;
    left: 4px;
}

/* IE7 Hack */
*+html #gabarit_tabmenu #tabs .first .submenu
{
    left: 0;
}

#gabarit_tabmenu #tabs .last .submenu
{
    left: -105px;
}

/* IE7 Hack */
*+html #gabarit_tabmenu #tabs .last:hover a + ul.submenu
{
    left: -105px;
    
}

#gabarit_tabmenu #tabs li .submenu li
{
    display: block;

    clear: both;

    border-bottom: 1px solid #CCCCCC;

    background-image: none;
    
    padding: 0;
    margin: 0;

    width: 100%;
    height: 20px;

    font-size: 0.8em;
    line-height: 2.1em;
}


#gabarit_tabmenu #tabs li .submenu li:hover
{
    background-image: url('./images/general/menu-item-background.png');
    background-repeat: repeat-x;
}

#gabarit_tabmenu #tabs li .submenu .last
{
    border-bottom: none;
}

#gabarit_tabmenu #tabs li .submenu li a
{
    color: #3964AF;
    white-space: nowrap;

    width: 175px;
    height: 100%;
    
    display: block;
    
    padding: 0 0.5em;
    margin: 0;

    background-image: none;
    
    text-align: left;
}

#gabarit_tabmenu #tabs li .submenu li:hover a
{
    color: #FFF;
    background-image: none;
}

#gabarit_tabmenu #tabs li .submenu li a:hover
{
    color: #FFF;
    text-decoration: none;
}

/* IE6 Hack */
* html #gabarit_tabmenu #tabs .item a
{
    margin-bottom: -8px;
}

#gabarit_tabmenu #tabs li .submenu li ul.submenu2
{
    display: none;
}

#gabarit_tabmenu #tabs li ul.submenu li:hover ul.submenu2
{
    display: block;
    position: absolute;

    top: 0;
    left: 185px;
    width: 185px;

    border: 1px solid #CCCCCC;
    border-bottom: 2px solid #3964AF;
    background-color: #F9F9F9;
}

#gabarit_tabmenu #tabs li .submenu li .submenu2 li a
{
    color: #3964AF;
    font-size: 10px;
}

#gabarit_tabmenu #tabs li .submenu li .submenu2 li a:hover
{
    color: #FFF;
}

#gabarit_topicmenu
{
    height: 32px;

    margin: 0;

    background-color: #638CDE;

    font-size: 14px;

    clear: left;
}

#gabarit_topicmenu div
{
    background-image: url('./images/general/topicmenu-right-background.png');
    background-repeat: no-repeat;
    
    float: right;

    height: 32px;
    width: 26px;
}

#gabarit_topicmenu #topics
{
    background-image: url('./images/general/topicmenu-left-background.png');
    background-repeat: no-repeat;

    padding-left: 5px;

    text-align: center;
    
    height: 32px;
}

#gabarit_topicmenu #topics li
{    
    display: inline;
    
    height: 32px;
}

#gabarit_topicmenu #topics li span
{
    display: inline-block;
    
    height: 32px;
    
    padding-left: 8px;
    
    background-image: url('./images/general/btg-inactif.gif');
    background-repeat: no-repeat;
    background-position: left;
}


#gabarit_topicmenu #topics li.selected span
{
    background-image: url('./images/general/btg-actif.gif');
}

#gabarit_topicmenu #topics li a
{
    font-size: 12px;
    font-weight: bold;

    color: #000;
    
    line-height: 26px;

    display: inline-block;
    margin-top: 3px;
    
    background-image: url('./images/general/intbt-inactif.gif');
    background-repeat: repeat-x;
}

#gabarit_topicmenu #topics li.selected a
{
    background-image: url('./images/general/intbt-actif.gif');
}

#gabarit_topicmenu #topics li a:hover
{
    color: #FFF;
    text-decoration: none;
}

#gabarit_topicmenu #topics li strong
{
    display: inline-block;

    height: 32px;
    
    padding-right: 8px;
    
    background-image: url('./images/general/btd-inactif.gif');
    background-repeat: no-repeat;
    background-position: right;
}

#gabarit_topicmenu #topics li.selected strong
{
    background-image: url('./images/general/btd-actif.gif');
}

#gabarit_topicmenu #topics li.selected
{
    color: #F3A11F;
}

#gabarit_resourcemenu
{
    height: 32px;

    text-align: center;

    margin: 0;
    
    clear: left;

    background-image: url('./images/general/resourcemenu-background.png');
    background-repeat: repeat-x;
}

/* IE6 Hack */
* html #gabarit_resourcemenu
{
    height: 42px;
}

#gabarit_resourcemenu div
{
    background-image: url('./images/general/resourcemenu-right-background.png');
    background-repeat: repeat-y;

    float: right;

